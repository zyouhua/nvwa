<?xml version="1.0" encoding="utf-8" ?>
<form startPosition="CenterParent"
      caption="打开文件"
      borderStyle ="FixedToolWindow">

  <size width="589" height="400" />

  <controls>

    <__t virstream="panel"
         controlName="panel1"
         autoScroll="false"
         dockStyle="Bottom">

      <size height="40" />

      <controls>

        <__t virstream="button"
             controlName="ok"
             text="确定"
             onClick="url://www.nvwa.com/implement/notepad\notepad.implement:notepad.implement.OpenFileOkCommand">

          <point x="415" y="8" />

        </__t>

        <__t virstream="button"
             controlName="cancel"
             text="取消"
             onClick="url://www.nvwa.com/implement/notepad\notepad.implement:notepad.implement.OpenFileCancelCommand">

          <point x="495" y="8" />

        </__t>

        <__t virstream="textLabel"
             controlName="textLabel2"
             text="网络地址：">

          <point x="8" y="16" />
          <size width="66" height="12" />

        </__t>

        <__t virstream="textBox"
             controlName="fileName"
             default="">

          <point x="74" y="9" />
          <size width="334" height="21" />

        </__t>

      </controls>

    </__t>

    <__t virstream="panel"
         controlName="panel2"
         autoScroll="false"
         dockStyle="Fill">

      <size height="71" />

      <controls>

        <__t virstream="listView"
             controlName="listView1"
             viewStyle="false"
             dockStyle="Fill"
             loadCommand="url://www.nvwa.com/implement/notepad\notepad.implement:notepad.implement.OpenFileDialogListViewLoadCommand"
             itemActivateCommand="url://www.nvwa.com/implement/notepad\notepad.implement:notepad.implement.OpenFileDialogListViewItemActivateCommand"
             selectedIndexChangedCommand="url://www.nvwa.com/implement/notepad\notepad.implement:notepad.implement.OpenFileDialogSelectedIndexChangedCommand"/>
      
      </controls>

    </__t>

  </controls>

</form>
